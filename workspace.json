{
  "version": 1,
  "projects": {
    "redux-app": {
      "root": "apps/redux-app",
      "sourceRoot": "apps/redux-app/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/redux-app",
            "index": "apps/redux-app/src/index.html",
            "main": "apps/redux-app/src/main.tsx",
            "polyfills": "apps/redux-app/src/polyfills.ts",
            "tsConfig": "apps/redux-app/tsconfig.app.json",
            "assets": [
              "apps/redux-app/src/favicon.ico",
              "apps/redux-app/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/redux-app/src/environments/environment.ts",
                  "with": "apps/redux-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "redux-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "redux-app:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/redux-app/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/redux-app"],
          "options": {
            "jestConfig": "apps/redux-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "redux-app-e2e": {
      "root": "apps/redux-app-e2e",
      "sourceRoot": "apps/redux-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/redux-app-e2e/cypress.json",
            "tsConfig": "apps/redux-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "redux-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "redux-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/redux-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "redux-app-state-store": {
      "root": "libs/redux-app/state/store",
      "sourceRoot": "libs/redux-app/state/store/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/redux-app/state/store/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/redux-app/state/store"],
          "options": {
            "jestConfig": "libs/redux-app/state/store/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "redux-app-state-reducers": {
      "root": "libs/redux-app/state/reducers",
      "sourceRoot": "libs/redux-app/state/reducers/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/redux-app/state/reducers/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/redux-app/state/reducers"],
          "options": {
            "jestConfig": "libs/redux-app/state/reducers/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "redux-app-state-action-creators": {
      "root": "libs/redux-app/state/action-creators",
      "sourceRoot": "libs/redux-app/state/action-creators/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/redux-app/state/action-creators/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/redux-app/state/action-creators"],
          "options": {
            "jestConfig": "libs/redux-app/state/action-creators/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "redux-app-state-hooks": {
      "root": "libs/redux-app/state/hooks",
      "sourceRoot": "libs/redux-app/state/hooks/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/redux-app/state/hooks/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/redux-app/state/hooks"],
          "options": {
            "jestConfig": "libs/redux-app/state/hooks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "redux-app-components": {
      "root": "libs/redux-app/components",
      "sourceRoot": "libs/redux-app/components/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/redux-app/components/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/redux-app/components"],
          "options": {
            "jestConfig": "libs/redux-app/components/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "redux-app-state-action-types": {
      "root": "libs/redux-app/state/action-types",
      "sourceRoot": "libs/redux-app/state/action-types/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/redux-app/state/action-types/**/*.ts"]
          }
        }
      }
    },
    "redux-app-state-actions": {
      "root": "libs/redux-app/state/actions",
      "sourceRoot": "libs/redux-app/state/actions/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/redux-app/state/actions/**/*.ts"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "none",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "none"
      },
      "library": {
        "style": "none",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "redux-app"
}
